{
    "name": "template-react",
    "description": "A Phaser 3 project template that demonstrates React communication and uses Vite for bundling.",
    "version": "1.1.1",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-react.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-react/issues"
    },
    "homepage": "https://github.com/phaserjs/template-react#readme",
    "keywords": [
        "phaser",
        "phaser3",
        "react",
        "vite"
    ],
    "scripts": {
        "dev": "vite --mode development",
        "build": "vite build --mode production",
        "build:dev": "vite build --mode development",
        "preview": "vite preview",
        "server:dev": "cross-env NODE_ENV=development node server/index.js",
        "server:prod": "cross-env NODE_ENV=production node server/index.js",
        "start:dev": "docker-compose -f docker-compose.yml --env-file .env.local up -d",
        "start:prod": "docker-compose -f docker-compose.yml --env-file .env.production up -d",
        "stop": "docker-compose down -v",
        "deploy": "npm run build && npm run start:prod",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext js,jsx --fix",
        "format": "prettier --write \"src/**/*.{js,jsx}\" \"server/**/*.js\""
    },
    "dependencies": {
        "@colyseus/monitor": "^0.16.6",
        "@colyseus/ws-transport": "^0.16.3",
        "@supabase/supabase-js": "^2.49.1",
        "axios": "^1.8.2",
        "bcrypt": "^5.1.1",
        "colyseus": "^0.16.3",
        "colyseus.js": "^0.16.9",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "jsonwebtoken": "^9.0.2",
        "node-fetch": "^3.3.2",
        "pg": "^8.13.3",
        "phaser": "^3.88.2",
        "prom-client": "^15.1.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "redis": "^4.7.0",
        "winston": "^3.17.0",
        "winston-loki": "^6.1.3"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "terser": "^5.28.1",
        "vite": "^5.3.1"
    }
}
